{
    "manifest_version": 3,
    "name": "狐狸助手",
    "version": "1.0",
    "description": "一个简单的谷歌浏览器插件示例",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "popup.js"
        ]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["WangZhan.txt"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  